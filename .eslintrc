{
    "extends": "airbnb",
    "plugins": [ "jest" ],
    "env": {
        "jest/globals": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-unused-vars": ["error", {"argsIgnorePattern": "next"}],
        "linebreak-style": "off",
        "react/jsx-one-expression-per-line": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true }],
        "react/prop-types": [0]
    }
}